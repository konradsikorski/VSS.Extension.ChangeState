 {
        "manifestVersion": 1,
        "id": "change-status",
        "version": "0.1.57",
        "name": "Change Status",
        "description": "Quickly change work item status from context menu",
        "publisher": "KonradSikorski",
        "targets": [
            {
                "id": "Microsoft.VisualStudio.Services"
            }
        ],
        "icons": {
            "default": "static/images/logo.png"
         },
        "scopes": [
            "vso.work",
            "vso.work_write"
        ],
        "categories": [
            "Plan and track"
        ],
        "tags": [
            "work item",
            "menu",
            "status"
        ],
        "repository": {
            "type": "git",
            "uri": "https://github.com/konradsikorski/VSS.Extension.ChangeStatus"
        },
        "branding": {
            "color": "rgb(220, 235, 252)",
            "theme": "light"
        },
        "content": {
            "details": {
                "path": "details.md"
            }
        },
        "contributions": [
            {
                "id": "changeStateMenu",
                "type": "ms.vss-web.action-provider",
                "description": "Change state selected items",
                "targets": [
                    "ms.vss-work-web.backlog-item-menu",
                    "ms.vss-work-web.work-item-context-menu"
                ],
                "properties": {
                    "name": "Change state",
                    "uri": "static/changeStatusMenu.html",
                    "registeredObjectId": "changeStateMenu"
                }
            }
        ],
        "files": [
            {
                "path": "static", "addressable": true
            },
            {
                "path": "src", "addressable": true
            },
            {
                "path": "sdk/scripts", "addressable": true
            }
        ]
    }